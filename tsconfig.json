{
	"extends": "@apify/tsconfig",
	"compilerOptions": {
		"baseUrl": ".",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ES2022",
		"outDir": "dist",
		"noUnusedLocals": false,
		"lib": ["DOM"],
		"paths": {
			"@lib/*": ["lib/*"],
			"@src/*": ["src/*"]
		}
	},
	"include": ["./src/**/*"]
}
