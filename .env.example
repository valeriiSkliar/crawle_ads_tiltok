# API Keys - Replace with your actual keys when running locally
# Do NOT commit actual keys to the repository
OPENAI_API_KEY="your-openai-api-key-here"
GEMINI_API_KEY="your-gemini-api-key-here"

# TikTok Credentials
TIKTOK_EMAIL="your-tiktok-email"
TIKTOK_PASSWORD="your-tiktok-password"

# Crawler Settings
CRAWLER_MAX_WAIT_TIME=100000
CRAWLER_CAPTCHA_TIMEOUT=300000
CRAWLER_MAX_CAPTCHA_ATTEMPTS=3
CRAWLER_HUMAN_DELAY_MIN=500
CRAWLER_HUMAN_DELAY_MAX=2000

# Storage Paths
PATH_SCREENSHOTS="storage/screenshots/"
PATH_DATA="storage/data/"

# Proxy Settings
PROXY_ENABLED=false
PROXY_URL=""

# Filter Settings
# Comma-separated values for arrays
FILTER_REGION="Algeria,United States"
FILTER_INDUSTRY="E-commerce"
# FILTER_OBJECTIVE="Conversion"
FILTER_PERIOD="Last 7 days"
FILTER_AD_LANGUAGE="English"
# FILTER_AD_FORMAT="Video"
# FILTER_LIKES="1k-5k"

# User Agent Settings
USER_AGENT_USE_CUSTOM=false
USER_AGENT_CUSTOM="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/115.0.0.0 Safari/537.36"
